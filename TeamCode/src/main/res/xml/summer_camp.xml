<Robot type="FirstInspires-FTC">
    <!-- Config for main 2024 Robot -->
    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Control Hub" port="173">
            <goBILDA5202SeriesMotor name="FL" port="0"/>-->
            <goBILDA5202SeriesMotor name="FR" port="1"/>-->
            <goBILDA5202SeriesMotor name="BL" port="2"/>-->
            <goBILDA5202SeriesMotor name="BR" port="3"/>-->
            <goBILDAPinpoint name="pinpoint" port="0" bus="0"/>
        </LynxModule>
        <LynxModule name="Expansion Hub 2" port="2">

            <goBILDA5202SeriesMotor name="pivot" port="0"/>
            <goBILDA5202SeriesMotor name="liftBottom" port="1"/>
            <goBILDA5202SeriesMotor name="liftTop" port="2"/>-->+

            <Servo name="clawServo" port="4"/>
            <Servo name="wrist" port="5"/>
            <Servo name="wristPivot" port="3"/>
            <Servo name="leftShoulderPivot" port="1"/>
            <Servo name="rightShoulderPivot" port="0"/>

            <RevColorSensorV3 name="sensor_color" port="1" bus="1"/>





        </LynxModule>
    </LynxUsbDevice>

    <EthernetDevice name="limelight" serialNumber = "EthernetOverUsb:eth0:172.29.0.25" port = "0" ipAddress = "172.29.0.1"></EthernetDevice>
</Robot>